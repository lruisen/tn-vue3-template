{
  "name": "tn-template",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "uniapp + tuniao-ui-vue3 项目初始化模板",
  "license": "MIT",
  "author": {
    "name": "Lruisen",
    "email": "lruisen@email.cn",
    "github": "https://github.com/lruisen"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0"
  },
  "scripts": {
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-baidu": "uni -p mp-baidu",
    "dev:mp-jd": "uni -p mp-jd",
    "dev:mp-kuaishou": "uni -p mp-kuaishou",
    "dev:mp-lark": "uni -p mp-lark",
    "dev:mp-qq": "uni -p mp-qq",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:quickapp-webview": "uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
    "build:app": "pnpm format:src && uni build -p app",
    "build:app-android": "pnpm format:src && uni build -p app-android",
    "build:app-ios": "pnpm format:src && ni build -p app-ios",
    "build:custom": "pnpm format:src && uni build -p",
    "build:h5": "pnpm format:src && uni build && esno ./build/scripts/postBuild.ts --output=h5",
    "build:h5:ssr": "pnpm format:src && uni build --ssr",
    "build:mp-alipay": "pnpm format:src && uni build -p mp-alipay",
    "build:mp-baidu": "pnpm format:src && uni build -p mp-baidu",
    "build:mp-jd": "pnpm format:src && uni build -p mp-jd",
    "build:mp-kuaishou": "pnpm format:src && uni build -p mp-kuaishou",
    "build:mp-lark": "pnpm format:src && uni build -p mp-lark",
    "build:mp-qq": "pnpm format:src && uni build -p mp-qq",
    "build:mp-toutiao": "pnpm format:src && uni build -p mp-toutiao",
    "build:mp-weixin": "pnpm format:src && uni build -p mp-weixin && esno ./build/scripts/postBuild.ts --output=mp-weixin",
    "build:quickapp-webview": "pnpm format:src && uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "pnpm format:src && uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "pnpm format:src && uni build -p quickapp-webview-union",
    "type-check": "vue-tsc --noEmit",
    "format:src": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,less}\""
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-components": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-h5": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3090820231120001",
    "@tuniao/tn-icon": "^1.9.0",
    "@tuniao/tn-style": "^1.0.19",
    "@tuniao/tnui-vue3-uniapp": "^1.0.21",
    "luch-request": "^3.1.1",
    "pinia": "2.0.36",
    "pinia-plugin-persistedstate": "3.2.1",
    "vue": "^3.5.8",
    "vue-i18n": "^10.0.2"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.12",
    "@dcloudio/uni-automator": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3090820231120001",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-3090820231120001",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3090820231120001",
    "@tuniao/eslint-config": "^1.0.5",
    "@types/node": "^22.5.5",
    "@uni-helper/vite-plugin-uni-manifest": "^0.2.7",
    "@vue/tsconfig": "^0.5.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.0",
    "esno": "^4.8.0",
    "fs-extra": "^10.1.0",
    "picocolors": "^1.1.0",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "unocss": "^0.62.4",
    "unocss-preset-weapp": "^0.62.2",
    "unplugin-auto-import": "^0.18.3",
    "vite": "4.5.5",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^2.1.6"
  }
}
